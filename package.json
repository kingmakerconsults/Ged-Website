{
  "name": "ged-website-tools",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:frontend": "vite --config vite.config.mts",
    "build:frontend": "vite build --config vite.config.mts",
    "preview:frontend": "vite preview --config vite.config.mts",
    "maps:fetch:us-regions": "node scripts/fetch-us-regions-svg.mjs",
    "start": "node backend/server.js",
    "start:prod": "cross-env NODE_ENV=production node backend/server.js",
    "harvest:science": "node tools/gov-image-harvester/index.mjs --subject Science",
    "harvest:socstud": "node tools/gov-image-harvester/index.mjs --subject \"Social Studies\"",
    "img:stats": "node tools/gov-image-harvester/inspect.mjs --stats",
    "img:keywords:sci": "node tools/gov-image-harvester/inspect.mjs --keywords Science",
    "img:verify": "node tools/gov-image-harvester/inspect.mjs --verify-paths",
    "validate:math": "node scripts/validate_math_quizzes.js",
    "test:math": "node tests/math_quizzes.test.js",
    "test:ti30xs": "node tests/ti30xs_engine.test.js",
    "quizzes:import-expanded": "node scripts/import_expanded_quizzes.js",
    "quizzes:build-frontend-expanded": "node scripts/build_frontend_expanded_bundle.cjs",
    "quizzes:integrate-frontend": "npm run quizzes:import-expanded ; npm run quizzes:build-frontend-expanded",
    "quizzes:build-extras": "node scripts/build_ss_extras_json.cjs",
    "quizzes:build-all": "npm run quizzes:build-extras ; npm run quizzes:build-json",
    "validate-quizzes": "node tests/quizValidation.test.js",
    "counts:premade": "node scripts/print_premade_counts.js",
    "counts:ss:everywhere": "node scripts/count_social_studies_everywhere.js",
    "report:question-housing": "node scripts/report_question_housing.mjs",
    "quizzes:build-json": "node scripts/build-quizzes.js",
    "quizzes:ensure-min": "node scripts/ensure_min_quiz_questions.js",
    "sanitize:math:katex": "node scripts/sanitize_math_katex.mjs",
    "audit:math:premade": "node scripts/audit_premade_math_quizzes.js",
    "audit:math:compute": "node scripts/audit_math_computation.mjs",
    "audit:tier:progression": "node scripts/audit_tier_progression.js",
    "audit:image:quality": "node scripts/audit_image_question_quality.cjs",
    "rectify:image:quality": "node scripts/rectify_image_question_issues.cjs",
    "rebuild:image:quality": "node scripts/rebuild_image_question_replacements.cjs",
    "audit:tier:remediation": "node scripts/build_tier_remediation_queue.js",
    "audit:tier:rewrite-packets": "node scripts/build_tier_rewrite_packets.js",
    "tier:apply-question-tiers": "node scripts/apply_question_tiers_to_public_quizzes.js",
    "tier:rebalance-question-tiers": "cross-env REBALANCE_QUESTION_TIERS=1 node scripts/apply_question_tiers_to_public_quizzes.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "docx": "^8.5.0",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^4",
    "file-type": "^19",
    "jsdom": "^24",
    "p-queue": "^7",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router-dom": "^6.30.2",
    "robots-parser": "^3",
    "sharp": "^0.33",
    "undici": "^6"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.0",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "vite": "^5.0.0"
  }
}
