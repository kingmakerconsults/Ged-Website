# Copy this file to `.env` in the backend directory and fill in the values.

JWT_SECRET=replace-with-strong-secret
ADMIN_BYPASS_ENABLED=false
ADMIN_BYPASS_HASH=replace-with-bcrypt-hash
GOOGLE_AI_API_KEY=your-google-ai-api-key
GOOGLE_API_KEY=your-google-generative-ai-key
OPENAI_API_KEY=your-openai-api-key
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
DATABASE_URL=postgres://username:password@localhost:5432/database
GEOMETRY_FIGURES_ENABLED=false
GEOMETRY_JSON_SANITIZER_ENABLED=true
GEOMETRY_MAX_DECIMALS=2
MATH_TWO_PASS_ENABLED=true
PORT=3001
